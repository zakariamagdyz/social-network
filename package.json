  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "build": "set NODE_ENV=production&&nodemon server.js",
    "debug": "ndb server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently  \"npm run server\"   \"npm run client\"",
    "prod": "concurrently  \"npm run build\"   \"npm run client\"",
    "h-build": "cd client && npm run build",
    "h-installClient": "cd client && npm  install",
    "heroku-postbuild": "npm run h-installClient && npm run install h-build"
  },
